    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "three": "^0.74.0",
    "trip.core": "^0.6.3",
    "trip.dom": "^0.4.0"
